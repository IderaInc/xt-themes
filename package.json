{
  "name": "xt-themes",
  "description": "Visualisation Themes for FusionCharts XT",
  "homepage": "http://www.fusioncharts.com/",
  "version": "0.0.4",
  "license": "MIT",
  "author": "FusionCharts Technologies  <mail@labs.fusioncharts.com>",
  "keywords": [
    "fusioncharts",
    "charts",
    "gauges",
    "maps",
    "themes"
  ],
  "preferGlobal": false,
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/fusioncharts/xt-themes.git"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "del": "^2.2.2",
    "google-closure-compiler": "^20160208.4.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-preprocess": "^2.0.0",
    "gulp-wrap": "^0.13.0",
    "jshint": "2.4.2",
    "metascript": "0.18.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "test": "develop/ci/test/test",
    "lint": "develop/ci/lint/lint",
    "dev:build": "NODE_ENV=development webpack",
    "prod:build": "NODE_ENV=production webpack",
    "build": "npm run prod:build",
    "drone": "develop/ci/drone/drone",
    "release": "develop/ci/release/release"
  },
  "jshintConfig": {
    "bitwise": false,
    "camelcase": true,
    "curly": true,
    "eqeqeq": false,
    "es3": false,
    "forin": false,
    "freeze": true,
    "immed": true,
    "indent": 2,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": false,
    "plusplus": false,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "trailing": true,
    "asi": false,
    "boss": true,
    "debug": false,
    "eqnull": true,
    "evil": false,
    "expr": true,
    "funcscope": false,
    "globalstrict": false,
    "iterator": false,
    "lastsemic": false,
    "laxbreak": false,
    "laxcomma": false,
    "loopfunc": false,
    "maxlen": 120,
    "multistr": false,
    "notypeof": false,
    "proto": false,
    "scripturl": false,
    "smarttabs": false,
    "shadow": false,
    "sub": false,
    "supernew": false,
    "browser": false,
    "devel": false,
    "node": true,
    "onevar": true,
    "globals": {
      "FusionCharts": true,
      "window": true
    }
  }
}
